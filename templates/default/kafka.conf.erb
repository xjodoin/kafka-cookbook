LoadPlugin java

<Plugin "java">
JVMARG "-Djava.class.path=/usr/share/collectd/java/collectd-api.jar:/usr/share/collectd/java/generic-jmx.jar"
LoadPlugin "org.collectd.java.GenericJMX"

    <Plugin "GenericJMX">

        <MBean "kafka/classes">
            ObjectName "java.lang:type=ClassLoading"
            InstancePrefix "kafka_java"
            #InstanceFrom ""

            <Value>
              Type "gauge"
              InstancePrefix "loaded_classes"
              #InstanceFrom ""
              Table false
              Attribute "LoadedClassCount"
            </Value>
        </MBean>

            # Time spent by the JVM compiling or optimizing.
        <MBean "kafka/compilation">
            ObjectName "java.lang:type=Compilation"
            InstancePrefix "kafka_java"
            #InstanceFrom ""

            <Value>
              Type "total_time_in_ms"
              InstancePrefix "compilation_time"
              #InstanceFrom ""
              Table false
              Attribute "TotalCompilationTime"
            </Value>
        </MBean>


        <MBean "kafka/memory">
            ObjectName "java.lang:type=Memory,*"
            InstancePrefix "kafka_java_memory"
            #InstanceFrom "name"
            <Value>
              Type "memory"
              InstancePrefix "heap-"
              #InstanceFrom ""
              Table true
              Attribute "HeapMemoryUsage"
            </Value>

            <Value>
              Type "memory"
              InstancePrefix "nonheap-"
              #InstanceFrom ""
              Table true
              Attribute "NonHeapMemoryUsage"
            </Value>
        </MBean>

        <MBean "kafka/memory_pool">
            ObjectName "java.lang:type=MemoryPool,*"
            InstancePrefix "kafka_java_memory_pool-"
            InstanceFrom "name"
            <Value>
              Type "memory"
              #InstancePrefix ""
              #InstanceFrom ""
              Table true
              Attribute "Usage"
            </Value>
        </MBean>

        <MBean "kafka/garbage_collector">
            ObjectName "java.lang:type=GarbageCollector,*"
            InstancePrefix "kafka_java_gc-"
            InstanceFrom "name"

            <Value>
              Type "invocations"
              #InstancePrefix ""
              #InstanceFrom ""
              Table false
              Attribute "CollectionCount"
            </Value>

            <Value>
              Type "total_time_in_ms"
              InstancePrefix "collection_time"
              #InstanceFrom ""
              Table false
              Attribute "CollectionTime"
            </Value>
        </MBean>


    <MBean "kafka/broker-topic-metrics">
        ObjectName "kafka.server:type=BrokerTopicMetrics"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "AllTopicsMessagesInPerSec"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "AllTopicsBytesInPerSec"
        </Value
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "AllTopicsBytesOutPerSec"
        </Value>
    </MBean>

    <MBean "kafka/request-metrics">
        ObjectName "kafka.network:type=RequestMetrics"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "{Produce|Fetch-consumer|Fetch-follower}-RequestsPerSec"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "{Produce|Fetch-Consumer|Fetch-Follower}-TotalTimeMs"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "{Produce|Fetch-Consumer|Fetch-Follower}-QueueTimeMs"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "{Produce|Fetch-Consumer|Fetch-Follower}-LocalTimeMs"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "{Produce|Fetch-Consumer|Fetch-Follower}-RemoteTimeMs"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "{Produce|Fetch-Consumer|Fetch-Follower}-ResponseSendTimeMs"
        </Value>
    </MBean>

    <MBean "kafka/consumer-fetcher-manager">
        ObjectName "kafka.consumer:type=ConsumerFetcherManager"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "([-.\w]+)-MaxLag"
        </Value>
    </MBean>

    <MBean "kafka/log-flush-stats">
        ObjectName "kafka.log:type=LogFlushStats"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "LogFlushRateAndTimeMs"
        </Value>
    </MBean>

    <MBean "kafka/replica-manager">
        ObjectName "kafka.server:type=ReplicaManager"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "counter"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "UnderReplicatedPartitions"
        </Value>
        <Value>
          Type "counter"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "PartitionCount"
        </Value>
        <Value>
          Type "counter"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "LeaderCount"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "ISRShrinksPerSec"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "ISRExpandsPerSec"
        </Value>
    </MBean>

    <MBean "kafka/replica-fetcher-manager">
        ObjectName "kafka.server:type=ReplicaFetcherManager"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "([-.\w]+)-MaxLag"
        </Value>
    </MBean>

    <MBean "kafka/producer-request-purgatory">
        ObjectName "kafka.server:type=ProducerRequestPurgatory"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "PurgatorySize"
        </Value>
    </MBean>

    <MBean "kafka/fetch-request-purgatory">
        ObjectName "kafka.server:type=FetchRequestPurgatory"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "PurgatorySize"
        </Value>
    </MBean>

    <MBean "kafka/fetcher-lag-metrics">
        ObjectName "kafka.server:type=FetcherLagMetrics"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "([-.\w]+)-ConsumerLag"
        </Value>
    </MBean>

    <MBean "kafka/controller">
        ObjectName "kafka.controller:type=KafkaController"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "counter"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "ActiveControllerCount"
        </Value>
    </MBean>

    <MBean "kafka/controller-stats">
        ObjectName "kafka.controller:type=ControllerStats"
        #InstancePrefix ""
        #InstanceFrom ""
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "LeaderElectionRateAndTimeMs"
        </Value>
        <Value>
          Type "gauge"
          #InstancePrefix ""
          #InstanceFrom ""
          Table false
          Attribute "UncleanLeaderElectionsPerSec"
        </Value>
    </MBean>


        #####################
        # Connection blocks #
        #####################
        <Connection>
          Host "<%= @options[:instance_uuid] %>"
          ServiceURL "service:jmx:rmi:///jndi/rmi://localhost:9999/jmxrmi"
          InstancePrefix "kafka"
          Collect "kafka/classes"
          Collect "kafka/compilation"
          Collect "kafka/memory"
          Collect "kafka/memory_pool"
          Collect "kafka/garbage_collector"
          Collect "kafka/broker-topic-metrics"
          Collect "kafka/request-metrics"
          Collect "kafka/consumer-fetcher-manager"
          Collect "kafka/log-flush-stats"
          Collect "kafka/replica-manager"
          Collect "kafka/replica-fetcher-manager"
          Collect "kafka/producer-request-purgatory"
          Collect "kafka/fetch-request-purgatory"
          Collect "kafka/fetcher-lag-metrics"
          Collect "kafka/controller"
          Collect "kafka/controller-stats"
        </Connection>
    </Plugin>
</Plugin>
